
.container .swiper{
  width: 100%;
  height: 300rpx;
}

.container .swiper image{
  width: 100%;
  height: 100%;
}

.container .songlist{
  margin-top: 30rpx;
}

.container .songlist .list_wrap{
  width: 95%;
  margin: 0 auto;
}

.container .songlist .list_wrap .list_title{
  font-size: 32rpx;
  color: #000;
  margin-bottom: 22rpx;
}

.container .songlist .list_wrap .lists{
  margin-top: 20rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.container .songlist .list_wrap .lists .list_item{
    width: 50%;
    padding-right: 8px;
    margin-bottom: 10px;
    overflow: hidden;
}


.container .footer{
  margin-top: 50rpx;
  text-align: center;
}

.container .footer image{
  width: 164rpx;
  height: 68rpx;
}

.container .footer text{
  display: block;
  font-size: 24rpx;
}